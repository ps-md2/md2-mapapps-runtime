{
    "Bundle-SymbolicName": "md2_formcontrols",
    "Bundle-Version": "1.0",
    "Bundle-Name": "MD2 Form Controls",
    "Bundle-Description": "FormControl factories that provide MD2 specific controls.",
    "Bundle-Localization": [],
    "Bundle-Main": "",
