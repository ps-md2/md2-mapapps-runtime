            }
        },
        {
            "name": "ImageFactory",
            "provides": "dataform.ControlFactory",
            "properties": {
                "dataformtype": "simpleimage"
            }
        },
        {
            "name": "GridPanelFactory",
            "provides": "dataform.ControlFactory",
            "properties": {
                "dataformtype": "md2gridpanel"
